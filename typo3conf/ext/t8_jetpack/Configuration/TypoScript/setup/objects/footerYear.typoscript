# get current year to replace [[YEAR]] (used in footer)
temp.CURRENTYEAR = TEXT
temp.CURRENTYEAR {
	data = date:U
	strftime = %Y
	dataWrap = |
}

# replace [[YEAR]] with the current year
# [[YEAR]] can be used in all textfields an will be replaced with the current year
page.10 {
	stdWrap.replacement {
		20 {
			search = [[YEAR]]
			replace < temp.CURRENTYEAR
		}
	}
}
