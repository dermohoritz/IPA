# news template paths
plugin.tx_news {
	view {
		templateRootPaths {
			100 = EXT:t8_jetpack/Resources/Private/Extensions/news/Templates/
			200 = EXT:t8_custom/Resources/Private/Extensions/news/Templates/
		}
		partialRootPaths {
			100 = EXT:t8_jetpack/Resources/Private/Extensions/news/Partials/
			200 = EXT:t8_custom/Resources/Private/Extensions/news/Partials/
		}
		layoutRootPaths {
			100 = EXT:t8_jetpack/Resources/Private/Extensions/news/Layouts/
			200 = EXT:t8_custom/Resources/Private/Extensions/news/Layouts/
		}

		# pagination template path
		widget.GeorgRinger\News\ViewHelpers\Widget\PaginateViewHelper.templateRootPath = EXT:t8_jetpack/Resources/Private/Extensions/fluid_styled_content/Templates/
	}
}
