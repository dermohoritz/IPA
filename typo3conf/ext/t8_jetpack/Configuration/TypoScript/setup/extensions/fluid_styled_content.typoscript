## Fluid Template Paths
lib.contentElement {
	templateRootPaths {
		100 = EXT:t8_jetpack/Resources/Private/Extensions/fluid_styled_content/Templates/
		200 = EXT:t8_custom/Resources/Private/Extensions/fluid_styled_content/Templates/
	}
	partialRootPaths {
		100 = EXT:t8_jetpack/Resources/Private/Extensions/fluid_styled_content/Partials/
		200 = EXT:t8_custom/Resources/Private/Extensions/fluid_styled_content/Partials/
	}
	layoutRootPaths {
		100 = EXT:t8_jetpack/Resources/Private/Extensions/fluid_styled_content/Layouts/
		200 = EXT:t8_custom/Resources/Private/Extensions/fluid_styled_content/Layouts/
	}
}

lib.stdheader = < lib.contentElement
lib.contentElement.variables {
	siteName = TEXT
	siteName.value = {site:websiteTitle}
	siteName.insertData = 1
}

## General Paging Template
config.tx_extbase.view.widget.TYPO3\CMS\Fluid\ViewHelpers\Widget\PaginateViewHelper.templateRootPath = EXT:t8_jetpack/Resources/Private/Extensions/fluid_styled_content/Templates/

## Banner-Template
tt_content {
	t8jetpack_banner < lib.contentElement
	t8jetpack_banner {
		templateName = Banner.html
		dataProcessing {
			10 = TYPO3\CMS\Frontend\DataProcessing\FilesProcessor
			10 {
				references.fieldName = assets
			}
		}
	}
}
