# Banner
# add Banner element to newContentElement Wizard
# icon is registered in ext_localconf.php
mod.wizards.newContentElement.wizardItems.common {
   elements {
      t8jetpack_banner {
         iconIdentifier = t8-icon-banner
         title = LLL:EXT:t8_jetpack/Resources/Private/Language/locallang.xlf:tca.ceBanner
         description =
         tt_content_defValues {
            CType = t8jetpack_banner
         }
      }
   }
   show := addToList(t8jetpack_banner)
}
